    <body>
		 <referenceBlock name="page.main.title">
               <action method="setPageTitle">
                  <argument translate="true" name="title" xsi:type="string">My saved carts</argument>
               </action>
         </referenceBlock>
		<referenceBlock name="sale.reorder.sidebar" remove="true"/>
    </body>
</page>
