<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Success Page</title>
    </head>
    <body>
        <referenceBlock name="checkout.success">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Dcw_OrderSuccess::success.phtml</argument>
            </action>
        </referenceBlock>
        <referenceBlock name="checkout.success.print.button">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Dcw_OrderSuccess::button.phtml</argument>
            </action>
        </referenceBlock>
    </body>
</page>
