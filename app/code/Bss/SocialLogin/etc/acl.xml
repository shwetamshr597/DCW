<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Bss_SocialLogin::manager" title="BSS Commerce: Social Menu" sortOrder="90">
                    <resource id="Bss_SocialLogin::log" title="Records Social Login" translate="title" sortOrder="130" />
                    <resource id="Bss_SocialLogin::configuration" title="Config System" sortOrder="52" />
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                          <resource id="Bss_SocialLogin::config_social_login" title="BSS Commerce: Social Configuration" sortOrder="30"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
