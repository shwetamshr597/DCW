<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route method="GET" url="/V1/Instagrampro/getConfigData">
        <service class="Magecomp\Instagrampro\Api\InstagramProManagementInterface" method="getConfigData"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>  

     <route method="GET" url="/V1/Instagrampro/getImageData">
        <service class="Magecomp\Instagrampro\Api\InstagramProManagementInterface" method="getImageData"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>  
    <route method="GET" url="/V1/Instagrampro/getProductPageImageData">
        <service class="Magecomp\Instagrampro\Api\InstagramProManagementInterface" method="getProductPageImageData"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route> 
</routes>